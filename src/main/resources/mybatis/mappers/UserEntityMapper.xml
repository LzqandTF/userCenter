<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yijiawang.web.platform.userCenter.dao.UserEntityMapper" >
  <resultMap id="BaseResultMap" type="com.yijiawang.web.platform.userCenter.po.UserEntity" >
    <id column="auto_id" property="autoId" jdbcType="BIGINT" />
    <result column="user_custom_id" property="userCustomId" jdbcType="VARCHAR" />
    <result column="wx_user_id" property="wxUserId" jdbcType="VARCHAR" />
    <result column="user_name" property="userName" jdbcType="VARCHAR" />
  </resultMap>
  
  <select id="getUserByUserId" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select 
    *
    from tb_user_info
    where user_custom_id = #{userCustomId,jdbcType=VARCHAR}
  </select>
</mapper>